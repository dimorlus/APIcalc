IDC_WINAPICALC ACCELERATORS
BEGIN
    VK_F1,          IDM_HELP_CONTENTS,      VIRTKEY, NOINVERT
    VK_HOME,        IDM_CTRL_HOME,          VIRTKEY, CONTROL
    "R",            IDM_CTRL_R,             VIRTKEY, CONTROL
    "S",            IDM_CTRL_S,             VIRTKEY, CONTROL
    "I",            IDM_CTRL_I,             VIRTKEY, CONTROL
    VK_OEM_4,       IDM_CTRL_LBRACKET,      VIRTKEY, CONTROL  // Ctrl+[
    VK_OEM_6,       IDM_CTRL_RBRACKET,      VIRTKEY, CONTROL  // Ctrl+]
    VK_OEM_PLUS,    IDM_CTRL_PLUS,          VIRTKEY, CONTROL  // Ctrl++
    VK_OEM_MINUS,   IDM_CTRL_MINUS,         VIRTKEY, CONTROL  // Ctrl+-
    VK_ADD,         IDM_CTRL_PLUS,          VIRTKEY, CONTROL  // Ctrl+Numpad+
    VK_SUBTRACT,    IDM_CTRL_MINUS,         VIRTKEY, CONTROL  // Ctrl+Numpad-
END


