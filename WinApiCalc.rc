IDC_WINAPICALC MENU
BEGIN
    POPUP "&Calc"
    BEGIN
        MENUITEM "Pas style",                   ID_CALC_PASSTYLE
        MENUITEM "Case sensetive",              ID_CALC_CASESENSETIVE
        MENUITEM "Forced float",                ID_CALC_FORCEDFLOAT
        POPUP "&Format..."
        BEGIN
            MENUITEM "Scientific",              ID_FORMAT_SCIENTIFIC
            MENUITEM "Normalized",              ID_FORMAT_NORMALIZED
            MENUITEM "Fraction",                ID_FORMAT_FRACTION
            MENUITEM "Computing",               ID_FORMAT_COMPUTING
            MENUITEM "Integer",                 ID_FORMAT_INTEGER
            MENUITEM "Unsigned",                ID_FORMAT_UNSIGNED
            MENUITEM "Hex",                     ID_FORMAT_HEX
            MENUITEM "Octal",                   ID_FORMAT_OCTAL
            MENUITEM "Binary",                  ID_FORMAT_BINARY
            MENUITEM "Char",                    ID_FORMAT_CHAR
            MENUITEM "Wide char",               ID_FORMAT_WIDECHAR
            MENUITEM "Date time",               ID_FORMAT_DATETIME
            MENUITEM "Unix time",               ID_FORMAT_UNIXTIME
            MENUITEM "Degreese",                ID_FORMAT_DEGREESE
            MENUITEM "String",                  ID_FORMAT_STRING
            MENUITEM "Inch",                    ID_FORMAT_INCH
            MENUITEM "All",                     ID_FORMAT_ALL
            MENUITEM "Auto",                    ID_FORMAT_AUTO
        END
        POPUP "&Binary width"
        BEGIN
            MENUITEM "8",                       ID_BINARYWIDTH_8
            MENUITEM "16",                      ID_BINARYWIDTH_16
            MENUITEM "24",                      ID_BINARYWIDTH_24
            MENUITEM "32",                      ID_BINARYWIDTH_32
            MENUITEM "48",                      ID_BINARYWIDTH_48
            MENUITEM "64",                      ID_BINARYWIDTH_64
            MENUITEM "binwide(64)",             ID_BINARYWIDTH_BINWIDE
        END
        MENUITEM SEPARATOR
        MENUITEM "View &variables",             ID_CALC_VIEWVARIABLES
        MENUITEM SEPARATOR
        MENUITEM "Esc minimized",               ID_CALC_ESCMINIMIZED
        MENUITEM "Opacity(100)",                ID_CALC_OPACITY
        MENUITEM "Exit",                        ID_CALC_EXIT
    END
END


